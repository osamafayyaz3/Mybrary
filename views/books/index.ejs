<h2>Search Books</h2>
<form action="/books" method="get">
    <label>Name</label>
    <input type="text" name="title" value="<%=searchOptions.title%>">
    <button type="submit">Search</button>
</form>
<br>
<% books.forEach(book => { %>
    <img height="150" width="100" src="<%= book.coverImagePath%>" alt="asdf">
    <hr>
<% }) %>