<h2>Search Books</h2>
<form action="/books" method="get">
    <label>Name</label>
    <input type="text" name="title" value="<%=searchOptions.title%>">
    <button type="submit">Search</button>
</form>
<br>
<% books.forEach(book => { %>
    <div><h3>Book Name: <%= book.title  %></h3>
        <h3>Page Count: <%= book.pageCount  %></h3>
       <h3>Date: <%= book.publishedDate %></h3>
    </div>
    <hr>
<% }) %>